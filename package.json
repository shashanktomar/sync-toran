{
  "name": "sync-toran",
  "version": "0.1.0",
  "description": "A sync-gateway companion to generate sync function using yaml files.",
  "main": "index.js",
  "repository": "https://github.com/shashanktomar/sync-toran.git",
  "author": "Shashank Tomar",
  "license": "MIT",
  "scripts": {
    "patchTestHelper": "babel-node tools/patchTestHelper.js",
    "pretranspile": "rimraf lib",
    "transpile": "babel src -d lib && npm run patchTestHelper",
    "build": "npm run lint && npm run typecheck && npm run transpile && npm run build:examples && npm run test",
    "build:examples": "babel-node tools/buildExamples.js",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "test": "npm run typecheck && jest",
    "test-watch": "jest --watch",
    "typecheck": "flow",
    "flowtyped": "flow-typed install"
  },
  "dependencies": {
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "js-object-pretty-print": "^0.3.0",
    "js-yaml": "^3.8.3",
    "lodash": "^4.17.4",
    "prettier": "^1.2.2",
    "replace-in-file": "^2.5.0",
    "stream-replace": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-plugin-flow-runtime": "^0.10.0",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^3.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-mocha": "^4.9.0",
    "eslint-plugin-promise": "^3.5.0",
    "flow-bin": "^0.44.2",
    "flow-runtime": "^0.10.0",
    "flow-type": "^1.0.1",
    "immutable": "^3.8.1",
    "jest": "^19.0.2",
    "regenerator-runtime": "^0.10.3",
    "rimraf": "^2.6.1",
    "webpack-node-externals": "^1.5.4"
  }
}
